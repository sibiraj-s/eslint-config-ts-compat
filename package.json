{
  "name": "eslint-config-ts-compat",
  "version": "1.0.1",
  "description": "Turns off all rules that conflicts with typescript-eslint",
  "main": "index.js",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/eslint-config-ts-compat",
  "bugs": "https://github.com/sibiraj-s/eslint-config-ts-compat/issues",
  "homepage": "https://github.com/sibiraj-s/eslint-config-ts-compat",
  "keywords": [
    "eslint",
    "typescript-eslint",
    "rule-conflicts",
