{
  "name": "eslint-config-ts-compat",
  "version": "1.0.2",
  "description": "Turns off all rules that conflicts with typescript-eslint",
  "author": "sibiraj-s",
  "license": "MIT",
  "main": "./index.js",
  "exports": "./index.js",
  "repository": "https://github.com/sibiraj-s/eslint-config-ts-compat",
  "bugs": "https://github.com/sibiraj-s/eslint-config-ts-compat/issues",
  "homepage": "https://github.com/sibiraj-s/eslint-config-ts-compat",
  "keywords": [
    "eslint",
    "typescript-eslint",
    "rule-conflicts",
    "disable-rules",
    "eslint-rules",
    "eslint-rules-conflicts"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "validate": "eslint . --ignore-path .gitignore --ext .js,.ts",
    "test": "npm run validate",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-pegasus": "^3.0.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "typescript": "^4.4.4"
  }
}
